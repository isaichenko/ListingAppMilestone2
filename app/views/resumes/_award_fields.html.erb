<div class="nested-fields">
	<div class="form-group">
	  <%= f.label :title %>
	  <%= f.text_field :title, class: 'form-control' %>
	</div>

	<div class="form-group">
	  <%= f.label :date_awarded %>
	  <%= f.date_field :date_awarded, class: 'form-control' %>
	</div>

	<div class="form-group">
	  <%= f.label :description %>
	  <%= f.text_area :description, row: 2, class: 'form-control' %>
	</div>

	<%= link_to_remove_association 'Remove Award', f, class: "badge badge-danger" %>
	<hr>
</div>